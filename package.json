{
  "name": "tictactoe",
  "version": "1.0.0",
  "description": "a basic ai for tic tac toe",
  "main": "index.js",
  "author": "Jai Sabir",
  "license": "MIT",
  "scripts": {
    "pretest": "tsc",
    "test": "jest",
    "tsc": "tsc",
    "start": "node src/main.js"
  },
  "dependencies": {
    "@types/jest": "^24.0.11",
    "jest": "^24.3.1",
    "ts-jest": "^24.0.0",
    "tsc": "^1.20150623.0",
    "typescript": "^3.3.3333"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.d.ts",
      "!node_modules/**/*"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "testURL": "http://localhost"
  }
}
